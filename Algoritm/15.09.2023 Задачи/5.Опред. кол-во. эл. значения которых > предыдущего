using System;
class HelloWorld {
  static void Main() {
    int n=Convert.ToInt32(Console.ReadLine());
    int a=Convert.ToInt32(Console.ReadLine());
    int kolvo=0;
    for (int i=0;i<n-1;i++)
    {
       int sled=Convert.ToInt32(Console.ReadLine());
       
       if (a<sled)
       {
           kolvo+=1;
       }
       a=sled;
    }
 Console.WriteLine(kolvo);
  }
}
